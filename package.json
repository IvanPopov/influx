    "electron": "electron ./shell.js",
    "details": "node ./dist/details.js",
    "deploy": "node ./dist/deploy.js",
    "build:shell": "webpack --config ./webpack.shell.js",
    "build:utils": "webpack --config ./webpack.utils.js",
    "build:web": "cross-env NODE_ENV=production APP_TARGET=web webpack --config ./webpack.sandbox.js",
    "build:electron": "npm run build:shell & cross-env NODE_ENV=production APP_TARGET=electron webpack --config ./webpack.sandbox.js",
    "dev:electron": "npm run build:shell & cross-env NODE_ENV=development APP_TARGET=electron webpack --config ./webpack.sandbox.js",
    "dev:web": "cross-env NODE_ENV=development APP_TARGET=web webpack --config ./webpack.sandbox.js",
    "dev-server:web": "cross-env NODE_ENV=development APP_TARGET=web webpack-dev-server --config ./webpack.sandbox.js",
    "dev-server:electron": "npm run build:shell & cross-env NODE_ENV=development APP_TARGET=electron webpack --watch --config ./webpack.sandbox.js",
    "docs:web": "typedoc --out dist/web/docs src",
    "pack:electron": "npx electron-builder -c ./electron-builder.yml",
    "clean:electron": "node -e \"let fs = require('fs-extra');fs.emptyDir('dist/electron');fs.emptyDir('release');\"",
